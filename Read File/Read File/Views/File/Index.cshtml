
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>



<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Age</td>
                @*//<td>@employee.Department</td>*@
            </tr>
        }
    </tbody>
</table>
